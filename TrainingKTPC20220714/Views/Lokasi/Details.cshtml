@model TrainingKTPC20220714.Models.Lokasi

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Lokasi</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
    </dl>

    <br />
    <br />

    @foreach(var bangunan in Model.bangunanList)
    {
        @bangunan.Name @bangunan.Alamat @bangunan.BilTingkat
        <br />
    }
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
